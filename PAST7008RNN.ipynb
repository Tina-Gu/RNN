    "# Separate each line of the input txt\n",
    "def split_lyrics_file(text_file):\n",
    "\ttext = open(text_file, encoding='utf-8').read()\n",
    "\ttext = text.split(\"\\n\")\n",
    "\twhile \"\" in text:\n",
    "\t\ttext.remove(\"\")\n",
    "\treturn text"
